{
  "name": "@mateothegreat/svelte5-forms",
  "version": "0.1.6",
  "type": "module",
  "svelte": "./index.js",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.js",
      "import": "./index.js",
      "svelte": "./index.js"
    }
  },
  "files": [

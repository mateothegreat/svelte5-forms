    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "svelte": "^5.20.5",
    "svelte-check": "^4.1.4",
    "svelte-package": "^0.1.0",
    "tailwind-merge": "^3.0.2",

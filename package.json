{
  "name": "@mateothegreat/svelte5-forms",
  "version": "0.0.28",
  "type": "module",
  "svelte": "./index.js",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./styles/app.pcss": "./public/styles/app.pcss"

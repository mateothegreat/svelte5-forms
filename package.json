    "svelte-check": "^4.1.5",
    "svelte-package": "^0.1.0",
    "typedoc": "^0.27.9",
    "typedoc-github-theme": "^0.2.1",
    "typedoc-plugin-extras": "^4.0.0",
    "typedoc-plugin-include-example": "^2.0.2",
    "typedoc-plugin-inline-sources": "^1.2.1",
    "typedoc-plugin-mermaid": "^1.12.0",
    "typedoc-plugin-pages": "^1.1.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.1"

{
  "name": "@mateothegreat/svelte5-forms",
  "version": "0.1.3",
  "type": "module",
  "svelte": "./index.js",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.js",
      "import": "./index.js",
      "svelte": "./index.js"
    },
    "./styles/app.pcss": "./public/styles/app.pcss"
  },
  "files": [
    "./**/*"
  ],
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateothegreat/svelte5-forms.git"
  },
  "keywords": [
    "svelte 5",
    "svelte 5 forms",
    "svelte 5 form library"
  ],
  "scripts": {
    "check": "exit 0;svelte-check --tsconfig ./tsconfig.json",
    "build": "npm run check && rimraf dist && cpx 'src/lib/**/*' dist && cpx package.json dist",
    "release": "svelte-package && cp package.json dist/package.json && cd dist && npm publish"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.13.7",
    "bits-ui": "1.3.5",
    "cpx": "^1.5.0",
    "phosphor-svelte": "^3.0.1",
    "prettier": "^3.5.2",
    "prettier-plugin-sort-json": "^4.1.1",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "svelte": "^5.20.5",
    "svelte-check": "^4.1.4",
    "svelte-package": "^0.1.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.9",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.1"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "lucide-svelte": "^0.477.0"
  }
}